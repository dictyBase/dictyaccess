{
  "name": "dictyaccess",
  "version": "0.0.0-semantically-released",
  "homepage": "/dictyaccess",
  "private": true,
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "tsc": "tsc",
    "lint": "eslint --ext .jsx,.js src/",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "semantic-release": "semantic-release",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "circos": "https://github.com/dictybase-playground/circosJS/tarball/master",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.5.2",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#10.2.1",
    "dicty-components-login": "github:dictyBase/dicty-components-login#3.1.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-hooks": "dictyBase/dicty-hooks#1.1.1",
    "fontsource-roboto": "^3.1.5",
    "graphql": "^15.5.0",
    "jwt-decode": "^3.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.13.1",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "react-svg-pan-zoom": "^2.16.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.21",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "5",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react-styleguidist": "^11.1.5",
    "react-test-renderer": "^16.14.0",
    "semantic-release": "^17.4.2",
    "typescript": "^4.2.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/common/@types/*.{ts,tsx}",
      "!src/common/constants/*.{ts,tsx}",
      "!src/common/graphql/*.{ts,tsx}",
      "!src/common/mocks/*.{ts,tsx}",
      "!src/**/*Config.{ts,tsx,sample.ts}",
      "!src/common/utils/icons.ts",
      "!src/app/styleguide/*.{ts,tsx}",
      "!src/common/styles/*.{js,jsx,ts,tsx}"
    ],
    "resetMocks": false
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier",
      "plugin:jest-dom/recommended"
    ],
    "plugins": [
      "prettier",
      "jest-dom"
    ],
    "rules": {
      "import/exports-last": [
        "error"
      ]
    }
  }
}
